received = input()
dict = {}
removed_space = ''
if ' ' in received:
  removed_space = received.split()
  removed_space = ''.join(removed_space)

for index in range(len(removed_space)):
  character = removed_space[index]
  if character in dict:
    dict[character] += 1
  else:
    dict[character] = 1

for key, value in dict.items():
  print(f'{key} -> {value}')
