first_number = int(input())
second_number = int(input())
third_number = int(input())
def smallest_num(some_numbers: list) -> int:
  return min(some_numbers)
smallest_element = smallest_num([first_number, second_number, third_number])
print(smallest_element)

#or 

num1 = int(input())
num2 = int(input())
num3 = int(input())

def find_smallest_num(num1, num2, num3):
  if num1 < num2 and num1 < num3:
    return num1
  elif num2 < num1 and num2 < num3:
    return num2
  else:
    return num3

smallest_num = find_smallest_num(num1, num2, num3)
print(smallest_num)
