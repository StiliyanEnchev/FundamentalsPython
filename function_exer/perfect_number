given_number = int(input())

def is_perfect_number_or_not(number: int):
  divisor_sum = 0
  for num in range(1, number):
    if number % num == 0:
      divisor_sum += num
  if divisor_sum == number:
    print('We have a perfect number!')
  else:
    print("It's not so perfect.")

is_perfect_number_or_not(given_number)
