num1 = int(input())
num2 = int(input())
num3 = int(input())

def sum_numbers(num1, num2):
  return num1 + num2

def substract(result, num3):
  return result - num3

def add_and_subtract(num1, num2, num3):
  result = sum_numbers(num1, num2)
  final_result = substract(result, num3)
  return final_result

print(add_and_subtract(num1, num2, num3))
